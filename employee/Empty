sudo apt install nodejs
sudo apt install npm
npm install --legacy-peer-deps
npm run build
sudo apt install apache2
suod mv build/* /var/www/html
sudo systemctl restart apache2
cd /var/www/html
sudo rm index.html
cd
mkdir CDS
sudo mv CDS /var/www/html
